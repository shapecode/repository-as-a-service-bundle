{
    "name": "shapecode/repository-as-a-service",
    "description": "Symfony2 Bundle which you can register repositories easily.",
    "keywords": [
        "shapecode",
        "repository",
        "service",
        "doctrine",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/repository-as-a-service-bundle",
    "support": {
        "email": "support@shapecode.de",
        "issues": "https://github.com/shapecode/repository-as-a-service-bundle/issues",
        "source": "https://github.com/shapecode/repository-as-a-service-bundle/releases",
        "wiki": "https://github.com/shapecode/repository-as-a-service-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/framework-bundle": "~2.8|~3.0",
        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/doctrine-bridge": "~2.8|~3.0",

        "doctrine/orm": "~2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\RasSBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\RasSBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
